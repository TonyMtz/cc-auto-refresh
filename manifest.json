{
  "name": "ConquerClub auto refresh",
  "version": "0.0.1",
  "manifest_version": 2,
  "description": "Auto refresh for ConquerClub",
  "homepage_url": "https://github.com/TonyMtz/cc-auto-refresh",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "default_locale": "en",
  "content_scripts": [
    {
      "matches": [
        "https://www.conquerclub.com/game.php?*",
        "http://www.conquerclub.com/game.php?*"
      ],
      "js": ["js/myscript.js"]
    }
  ]
}
